/* https://practice.geeksforgeeks.org/problems/binary-number-to-decimal-number3525/1 */

//code--------------------------------------------------


class Solution
{
	public:
		int binary_to_decimal(string str)
		{
		    // Code here
		    int d=0;
		    int len=str.length();
		    for(int i=0;i<len;i++)
		    {
		        if(str[i]=='1')
		       d+=pow(2,len-i-1);
		    }
		    return d;
        }

};
