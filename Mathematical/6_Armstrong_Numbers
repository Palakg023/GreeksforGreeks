/* https://practice.geeksforgeeks.org/problems/armstrong-numbers2727/1 */

//code---------------------------------------------------------------------------

class Solution {
  public:
    string armstrongNumber(int n){
        // code here
        int sum=0;
        int a=n;
        while(n>0)
        {
            int rem=n%10;
            sum=sum+pow(rem,3);
            n/=10;
        }
        if(sum==a)
        return "Yes";
        else
        return "No";
    }
};
