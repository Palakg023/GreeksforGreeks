/* https://practice.geeksforgeeks.org/problems/print-the-kth-digit3520/1# */

//code-----------------------------------------------------------------

class Solution{
public:
    int kthDigit(int A,int B,int K){
        // code here
        long long C=pow(A,B);
        int d;
        while(K>0)
        {
            d=C%10;
            C/=10;
            K--;
        }
        return d;
    }
};
